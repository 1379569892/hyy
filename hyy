function ob5 {
iptables -F 
iptables -X 
iptables -Z
ip6tables -F
ip6tables -X
ip6tables -Z
iptables -P OUTPUT ACCEPT
iptables -P INPUT ACCEPT
clear

printf "\n\033[32;1mhyy\033[0m\n"

}

function ob6 {

clear

printf "\n\033[32;1mhyy\033[0m\n"

}

function ob7 {

clear

printf "\n\033[32;1mhyy\033[0m\n"

}

function ob8 {

clear

printf "\n\033[32;1mhyy\033[0m\n"

}


function menu {

clear

echo

echo -e "\n\n\n\t\t$obi hyy专属sh\n"

echo -e "$OK\t1. 大厅开启（第一次防举报）$NO"

echo -e "$OK\t2. 预防高风险清理$NO"

echo -e "$OK\t3. 上号前的游戏清理$NO"

echo -e "$OK\t4. 大厅开启（第二次防举报）$NO"

echo -e "$OK\t5. 下线必须开启$NO"

echo -e "$OK\t6. 未开发$NO"

echo -e "$OK\t7. 未开发$NO"

echo -e "$OK\t8. 未开发$NO"

echo -e "$KO\t0. 退出程序$NO\n\n"

echo -en "$OK\t请输入序号，选择您要执行的操作: $NO"

read -n 1 option

}



#主程序

while [ 1 ]

do

menu

case $option in

0)

break ;;

1)

ob1 ;;

2)

ob2 ;;

3)

ob3 ;;

4)

ob4 ;;

5)

ob5 ;;

6)

ob6 ;;

7)

ob7 ;;

8)

ob8 ;;


*)

clear

echo "$KO你选择了错误的序号$NO";;

esac

echo -en "$OK\n\n\t\t选择任意键返回菜单$NO"

read -n 1 line

done

clear
