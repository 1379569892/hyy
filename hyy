                
obi='\e[1;33m'
KO='\e[1;31m'
OK='\e[1;32m'
NO='\e[0m'

function ob1 {   
while true
do
echo 16384 > "/proc/sys/fs/inotify/max_queued_events"
sleep 0.001
echo 128 > "/proc/sys/fs/inotify/max_user_instances"
sleep 0.001
echo 8192 > "/proc/sys/fs/inotify/max_user_watches"
sleep 0.001


 set +x
    let "j=j+1"
   echo -e "\033[35;;1m hyy防举报：$j 次！ \033[0m" 
  done

clear

printf "\n\033[32;1mhyy专属sh执行成功\033[0m\n"

}

