echo -e "$OK\t6. 未开发$NO"

echo -e "$OK\t7. 未开发$NO"

echo -e "$OK\t8. 未开发$NO"

echo -e "$KO\t0. 退出程序$NO\n\n"

echo -en "$OK\t请输入序号，选择您要执行的操作: $NO"

read -n 1 option

}



#主程序

